model EmergencyNews {
  id       Int             @id @default(autoincrement())
  image    String?
  title_uz String?
  title_en String?
  title_ru String?
  title_oz String?
  desc_uz  String?
  desc_en  String?
  desc_ru  String?
  desc_oz  String?
  for      String?
  parent   EmergencyNews?  @relation("EmergencyNewsRelation", fields: [parentId], references: [id])
  children EmergencyNews[] @relation("EmergencyNewsRelation")
  parentId Int?
}

enum EmergencyStatusEnum {
  PENDING
  DONE
}

