  question_desc_ru       String?
  question_desc_en       String?
  question_desc_oz       String?
  age_from               Int?
  age_to                 Int?
  gender                 GenderEnum?
  disability             Boolean?
  citizenship            CitizenshipEnum?
  martial_status         MartialStatusEnum?
  presence_of_children   Boolean?
  education              EducationTypeEnum?
